
.tab{display: -webkit-box;display: -webkit-flex;display: flex;height:100%;}
.left-aside{width:200rpx; -webkit-flex-shrink: 0; flex-shrink: 0;height: 100%;background-color: #fff;}
.left-aside .f-item{text-align: center;color: #606266;border-bottom: 1px solid #E4E7ED;height: 48px;line-height: 48px;position: relative;}
.left-aside .active:before {
    content: '';
    position: absolute;
    left: 0;
    top: 50%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    height: 17px;
    width: 3px;
    background-color: #D43D37;
    border-radius: 0 4px 4px 0;
    opacity: .8;
}
.right-aside{-webkit-box-flex: 1;-webkit-flex: 1;flex: 1;padding-left: 20rpx;overflow-y: auto;overflow: hidden;}
.s-list{background-color: #fff;min-height: 100%;}

